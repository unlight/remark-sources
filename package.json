{
  "name": "remark-sources",
  "version": "1.0.2",
  "description": "Insert source code to markdown files",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test:cov": "c8 --reporter text --exclude \"**/*.spec.js\" npm run test:r",
    "test:d": "node --inspect-brk src/index.spec.js",
    "test:w": "watchexec --watch src --ignore node_modules \"npm run test:r\"",
    "test:r": "pta --no-esm src/index.spec.js",
    "test": "npm run test:cov",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "build": "sh Taskfile build"
  },
  "dependencies": {
    "njct": "^8.0.0",
    "unist-util-visit": "^2.0.3"
  },
  "peerDependencies": {
    "remark": ">=13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "c8": "^7.3.1",
    "husky": "^4.3.0",
    "precise-commits": "^1.0.2",
    "prettier": "^2.1.2",
    "pta": "^0.1.3",
    "remark": "^13.0.0-alpha.1",
    "semantic-release": "^17.1.2",
    "watchexec-bin": "^1.0.0"
  },
  "keywords": [
    "remark-plugin",
    "markdown",
    "remark"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/remark-sources.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/remark-sources/issues"
  },
  "homepage": "https://github.com/unlight/remark-sources#readme"
}
